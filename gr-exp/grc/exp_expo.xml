<?xml version="1.0"?>
<block>
  <name>expo</name>
  <key>exp_expo</key>
  <category>Exponential</category>
  <import>import gnuradio.expo</import>
  <make>gnuradio.expo.expo()
self.$(id).set_parameters($q,$e,$wd)
  </make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
<!--  <param>
    <name>...</name>
    <key>...</key>
    <type>...</type>
  </param>-->

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
       <param>
        <name>Sigma Value</name>
        <key>q</key>
        <value>0</value>
        <type>real</type>
    </param>
    <param>
        <name>Zita Value</name>
        <key>e</key>
        <value>0</value>
        <type>real</type>
    </param>
    <param>
        <name>Frequency</name>
        <key>wd</key>
        <value>0</value>
        <type>real</type>
    </param>
  <sink>
    <name>in</name>
    <type>float</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>float</type>
  </source>
  <doc>
	  Enter the sigma value real value.
	  Enter the zita value damping factor(i.e less than 0,greater than 0 or equal to zero)
	  Enter the damping frequency
 </doc>
</block>
